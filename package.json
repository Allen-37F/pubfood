{
  "name": "pubfood",
  "version": "0.0.3",
  "description": "Header bidding library",
  "license": "MIT",
  "author": {
    "name": "pubfood and other contributors",
    "url": "https://github.com/pubfood/pubfood/blob/master/AUTHORS.txt"
  },
  "main": "./src/pubfood.js",
  "scripts": {
    "prepare": "npm install && mkdir -p reports",
    "clean": "npm run prepare && gulp clean",
    "lint": "gulp lint",
    "test": "gulp test",
    "docco": "docco --output=dist/doc examples/literate.js",
    "doc": "gulp doc",
    "create_diagram": "jsdoc -t node_modules/jsdoc2diagram -r ./src -d ./dist/doc",
    "build": "npm run prepare && npm run lint && gulp build && npm run test && npm run doc && npm run create_diagram",
    "start": "npm run build && gulp serve",
    "copy_lib": "cp dist/pubfood.* standalone/public",
    "run_standalone": "npm run copy_lib && node standalone/lib/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pubfood/pubfood.git"
  },
  "keywords": [
    "prebid",
    "header",
    "bidding",
    "auction",
    "advertising",
    "async",
    "parallel"
  ],
  "bugs": {
    "url": "https://github.com/pubfood/pubfood/issues"
  },
  "homepage": "http://pubfood.org",
  "browserify": {
    "standalone": "pubfood",
    "entries": [
      "./src/pubfood.js"
    ]
  },
  "devDependencies": {
    "boom": "2.9.0",
    "browser-sync": "^2.9.4",
    "browserify": "^11.1.0",
    "chai": "^3.3.0",
    "del": "^2.0.2",
    "eventemitter3": "^1.1.1",
    "gulp": "^3.9.0",
    "gulp-connect": "^2.2.0",
    "gulp-eslint": "^1.0.0",
    "gulp-header": "^1.7.1",
    "gulp-if": "^2.0.0",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-mocha": "^2.1.3",
    "gulp-mocha-phantomjs": "^0.10.1",
    "gulp-open": "^1.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-shell": "^0.4.3",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^1.4.1",
    "hapi": "9.3.1",
    "inert": "3.1.0",
    "jsdoc": "^3.3.3",
    "jsdoc2diagram": "0.0.6",
    "mocha": "^2.3.3",
    "vinyl-source-stream": "^1.1.0"
  }
}
