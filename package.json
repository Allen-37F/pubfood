{
  "name": "pubfood",
  "version": "0.1.2",
  "description": "Header bidding library",
  "license": "MIT",
  "author": {
    "name": "pubfood and other contributors",
    "url": "https://github.com/pubfood/pubfood/blob/master/AUTHORS.txt"
  },
  "main": "./src/pubfood.js",
  "scripts": {
    "prepare": "npm install",
    "clean": "npm prune --production && npm run prepare && gulp clean",
    "lint": "gulp lint",
    "test": "gulp test",
    "docco": "docco --output=build/doc examples/literate.js",
    "doc": "gulp doc",
    "create_diagram": "jsdoc -t node_modules/jsdoc2diagram -r ./src -d ./build/doc",
    "build": "npm run prepare && npm run lint && gulp build && npm run test",
    "copy_build_to_dist": "gulp build && cp build/pubfood.* dist",
    "bump-version": "npm --no-git-tag-version version patch && npm run copy_build_to_dist && git commit . -m 'copy build files to dist' && git tag `node -e 'console.log(require(\"./package.json\").version)'`",
    "start": "npm run build && gulp serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pubfood/pubfood.git"
  },
  "keywords": [
    "prebid",
    "header",
    "bidding",
    "auction",
    "advertising",
    "async",
    "parallel"
  ],
  "bugs": {
    "url": "https://github.com/pubfood/pubfood/issues"
  },
  "homepage": "http://pubfood.org",
  "browserify": {
    "standalone": "pubfood",
    "entries": [
      "./src/pubfood.js"
    ]
  },
  "devDependencies": {
    "browser-sync": "^2.9.4",
    "browserify": "^11.1.0",
    "chai": "^3.3.0",
    "del": "^2.0.2",
    "eventemitter3": "^1.1.1",
    "gulp": "^3.9.0",
    "gulp-mocha-phantomjs": "^0.10.1",
    "gulp-mocha": "^2.1.3",
    "gulp-eslint": "^1.0.0",
    "gulp-shell": "^0.4.3",
    "gulp-replace": "^0.5.4",
    "gulp-streamify": "^1.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.4.1",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "jsdoc": "^3.3.3",
    "jsdoc2diagram": "0.0.6",
    "vinyl-source-stream": "^1.1.0"
  }
}
